<Window x:Class="Solution_lab8.SetRowsColumns"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Game Changer" Height="166" Width="269">
    <Grid>
        <Label Content="Set Maximum Rows and Columns:" Height="28" HorizontalAlignment="Left" Name="label1" VerticalAlignment="Top" Width="189" />
        <Label Content="Rows" Height="28" HorizontalAlignment="Left" Margin="12,34,0,0" Name="label2" VerticalAlignment="Top" Padding="0" Width="37" />
        <Label Content="Columns" Height="28" HorizontalAlignment="Left" Margin="151,34,0,0" Name="label3" VerticalAlignment="Top" Padding="0" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,53,0,0" Name="textBox1" VerticalAlignment="Top" Width="84" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="151,53,0,0" Name="textBox2" VerticalAlignment="Top" Width="84" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="12,92,0,0" Name="button1" VerticalAlignment="Top" Width="84" Click="button1_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="151,92,0,0" Name="button2" VerticalAlignment="Top" Width="84" Click="button2_Click" />
    </Grid>
</Window>
